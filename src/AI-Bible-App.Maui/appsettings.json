{
  "Ollama": {
    "Url": "http://localhost:11434",
    "ModelName": "phi4:latest",
    "EmbeddingModel": "nomic-embed-text",
    "NumCtx": 4096,
    "NumPredict": 1024,
    "NumGpu": -1,
    "NumThread": 0,
    "PrewarmOnStartup": true
  },
  "Groq": {
    "ApiKey": "",
    "ModelName": "llama-3.1-8b-instant"
  },
  "AI": {
    "PreferLocal": true
  },
  "RAG": {
    "Enabled": true,
    "ChunkingStrategy": "SingleVerse"
  },
  "Features": {
    "ContextualReferences": true
  },
  "Bible": {
    "DataDirectory": "Data/Bible",
    "Translations": "web,asv,ylt",
    "DefaultTranslation": "WEB"
  },
  "Caching": {
    "SystemPromptCacheMinutes": 60,
    "RAGContextCacheMinutes": 30,
    "MaxRAGCacheEntries": 200,
    "ConversationResponseCacheEnabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  }
}
